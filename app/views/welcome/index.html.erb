<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<%= link_to 'List Item', new_item_path %>

<table>
	<tr>
		<th>Title</th>
		<th> </th>
	</tr>

	<% @items.each do |item| %>
		<tr>
			<td><%= item.title%> </td>
			<td><%= link_to 'Show', item_path(item) %> </td>
		</tr>
	<% end %>
</table>